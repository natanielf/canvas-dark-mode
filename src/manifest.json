{
    "manifest_version": 3,
    "name": "Canvas Dark Mode",
    "version": "0.1.0",
    "description": "Adds dark mode to Canvas LMS",
    "permissions": [
    "storage"
  ],
    "host_permissions": [
    "https://*.instructure.com/"
  ],
    "content_scripts": [
        {
            "matches": [
        "https://*.instructure.com/*"
      ],
            "css": [
        "css/style.css"
      ],
            "js": [
        "js/content.js"
      ]
    }
  ]
}
