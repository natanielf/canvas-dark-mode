{
  "name": "Canvas Dark Mode",
  "version": "0.1.0",
  "description": "Adds dark mode to Canvas LMS",
  "permissions": [
  "storage",
   "tabs"
  ],
  "host_permissions": [
  "https://*.instructure.com/"
  ],
  "background": {
    "scripts": [
    "js/background.js"
    ],
    "persistent": false
  },
   "content_scripts": [
    {
      "matches": [
        "https://*.instructure.com/",
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
  "manifest_version": 3
}
